{
  "name": "BVBMedia",
  "version": "1.2.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:local",
    "start:local": "ng serve --configuration=local --o --host 0.0.0.0",
    "start:local:nl": "ng serve --configuration=local-nl --o --host 0.0.0.0",
    "start:dev": "ng serve --configuration=development --o --host 0.0.0.0",
    "start:dev:nl": "ng serve --configuration=development-nl --o --host 0.0.0.0",
    "start:prod": "ng serve --configuration=production --o --host 0.0.0.0",
    "start:prod:nl": "ng serve --configuration=production-nl --o --host 0.0.0.0",
    "build:local": "ng build --configuration=local && sw-precache --root=dist/en --config=precache-config.js && gulp --language=en --env=local",
    "build:local:nl": "ng build --configuration=local-nl && sw-precache --root=dist/nl --config=precache-config-nl.js && gulp --language=nl --env=local",
    "build:dev": "ng build --configuration=development && sw-precache --root=dist/en --config=precache-config.js && gulp --language=en --env=dev",
    "build:dev:nl": "ng build --configuration=development-nl && sw-precache --root=dist/nl --config=precache-config-nl.js && gulp --language=nl --env=dev",
    "build:prod": "ng build --configuration=production && sw-precache --root=dist/en --config=precache-config.js && gulp --language=en --env=prod",
    "build:prod:nl": "ng build --configuration=production-nl && sw-precache --root=dist/nl --config=precache-config-nl.js && gulp --language=nl --env=prod",
    "serve:sw:local": "npm run build:local && npx http-server ./dist/en -p 8000 --gzip",
    "serve:sw:local:nl": "npm run build:local:nl && npx http-server ./dist/nl -p 8001 --gzip",
    "serve:sw:dev": "npm run build:dev && npx http-server ./dist/en -p 8000 --gzip",
    "serve:sw:dev:nl": "npm run build:dev:nl && npx http-server ./dist/nl -p 8001 --gzip",
    "serve:sw:prod": "npm run build:prod && npx http-server ./dist/en -p 8000 --gzip",
    "serve:sw:prod:nl": "npm run build:prod:nl && npx http-server ./dist/nl -p 8001 --gzip",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract": "ng xi18n --output-path=locale --i18n-locale en && npm run xliffmerge",
    "xliffmerge": "xliffmerge --profile xliffmerge.json nl",
    "postbuild:dev": "scp -r dist/en/. root@deb1.bvb.vg:/var/www/plattetv.bvb.media/web/",
    "postbuild:dev:nl": "scp -r dist/nl/. root@deb1.bvb.vg:/var/www/plattetv.bvb.media/web/",
    "postbuild:prod": "scp -r dist/en/. root@ct1node1.retailenclicks.nl:/var/www/plattetvnl.ct1node1.retailenclicks.nl/web/",
    "postbuild:prod:nl": "scp -r dist/nl/. root@ct1node1.retailenclicks.nl:/var/www/plattetvnl.ct1node1.retailenclicks.nl/web/",
    "generate-debug-report": "ng build --prod --build-optimizer --aot --named-chunks --stats-json && gulp && sw-precache --root=dist/en --config=precache-config.js",
    "view-debug-report": "webpack-bundle-analyzer dist/nl/stats.json"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "1.0.0-beta.3",
    "@angular/animations": "6.0.3",
    "@angular/cdk": "6.2.0",
    "@angular/common": "6.0.3",
    "@angular/compiler": "6.0.3",
    "@angular/core": "6.0.3",
    "@angular/flex-layout": "5.0.0-beta.14",
    "@angular/forms": "6.0.3",
    "@angular/http": "6.0.3",
    "@angular/material": "6.2.0",
    "@angular/platform-browser": "6.0.3",
    "@angular/platform-browser-dynamic": "6.0.3",
    "@angular/pwa": "^0.8.1",
    "@angular/router": "6.0.3",
    "@angular/service-worker": "6.0.3",
    "@fortawesome/angular-fontawesome": "^0.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@ncstate/sat-popover": "^2.1.1",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/entity": "^6.1.0",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngrx/store-devtools": "^6.1.0",
    "@types/googlemaps": "^3.30.12",
    "angular-local-storage": "^0.7.1",
    "angular-localstorage": "^1.1.5",
    "core-js": "2.5.7",
    "gulp-jsdom": "^1.0.1",
    "hammerjs": "2.0.8",
    "imgix.js": "^3.3.2",
    "ngrx-store-freeze": "^0.2.4",
    "ngx-lazy-load-images": "^1.3.1",
    "ngx-pagination": "3.1.1",
    "ngx-perfect-scrollbar": "6.1.0",
    "ngx-swiper-wrapper": "6.2.1",
    "rxjs": "6.2.0",
    "rxjs-compat": "6.2.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.7",
    "@angular/cli": "6.0.7",
    "@angular/compiler-cli": "6.0.3",
    "@angular/language-service": "6.0.3",
    "@ngrx/schematics": "^6.1.0",
    "@types/jasmine": "2.8.7",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "10.1.4",
    "codelyzer": "4.3.0",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-gzip": "^1.4.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-minify-css": "^1.2.4",
    "gulp-string-replace": "^1.1.1",
    "gulp-uglify": "^3.0.1",
    "gulp-uglify-es": "^1.0.4",
    "imagemin-cli": "^3.0.0",
    "imagemin-cli-preserve-dir-structure": "0.0.2",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.1.0",
    "ngx-i18nsupport": "^0.17.0",
    "protractor": "^5.4.1",
    "shelljs": "^0.8.2",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-node": "6.0.5",
    "tslint": "5.10.0",
    "typescript": "2.7.2",
    "webpack-bundle-analyzer": "^2.13.1"
  }
}
